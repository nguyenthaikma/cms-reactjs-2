{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@src": "./src",
          "@assets": "./assets",
          "@components": "./src/components",
          "@configs": "./src/configs",
          "@hooks": "./src/hooks",
          "@libs": "./src/libs",
          "@modules": "./src/modules",
          "@pages": "./src/pages",
          "@queries": "./src/queries",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}
